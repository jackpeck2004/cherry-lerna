{
  "name": "@cherry-challenge/file-publisher",
  "version": "0.0.0-development",
  "private": true,
  "description": "splits files to feed to kafka",
  "main": "src/app.ts",
  "scripts": {
    "start": "tsc -p . && node ./build/app.js --trace-warnings",
    "start:watch": "nodemon",
    "build": "tsc -p .",
    "lint": "eslint .",
    "semantic-release": "semantic-release",
    "build:dev": "rimraf ./build && tsc -p .",
    "test": "tsc -b && jest --forceExit --coverage --detectOpenHandles",
    "test:watch": "nodemon --watch \"src/**\" --exec \"tsc -b && jest --forceExit --coverage --detectOpenHandles\""
  },
  "dependencies": {
    "@supercharge/promise-pool": "^1.7.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "cross-blob": "^3.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "express": "^4.17.1",
    "kafkajs": "^1.15.0",
    "moment": "^2.29.1",
    "mongodb": "^3.6.8",
    "multer": "^1.4.2",
    "nanoid": "^3.1.23"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/config": "^0.0.39",
    "@types/cors": "^2.8.12",
    "@types/jest": "^27.0.3",
    "@types/mongodb": "^3.6.16",
    "@types/multer": "^1.4.7",
    "nodemon": "^2.0.12",
    "semantic-release": "^17.4.4",
    "ts-node": "^10.1.0"
  },
  "author": "Cherry srl",
  "license": "ISC"
}
