{
 "compilerOptions": {
   "module": "commonjs",
   "declaration": true,
   "noImplicitAny": false,
   "removeComments": true,
   "noLib": false,
   "emitDecoratorMetadata": true,
   "experimentalDecorators": true,
   "target": "es6",
   "allowJs": true,
   "esModuleInterop": true,
   "sourceMap": true,
   "lib": [
     "es6",
     "DOM",
     "es2019"
   ]
 },
 "exclude": [
   "node_modules",
   "**/*.spec.ts"
 ]
}

